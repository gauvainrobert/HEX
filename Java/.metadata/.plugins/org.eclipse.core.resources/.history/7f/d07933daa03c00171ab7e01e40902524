package hex.ihm.vue;

import java.awt.Graphics;
import javax.swing.JPanel;
 
public class Panneau extends JPanel {
  public void paintComponent(Graphics g){                
  	int x[] = {20, 30, 50, 60, 60, 50};
    int y[] = {30, 20, 20, 30, 50, 60};
    g.drawPolygon(x, y, 6);
  }               
}
